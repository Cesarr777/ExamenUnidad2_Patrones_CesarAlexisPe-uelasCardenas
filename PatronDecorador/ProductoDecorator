//proyecto decorador
namespace EbayShopConsole
{
    /// <summary>
    /// Clase base para los decoradores de producto (descuento, envío, etc.)
    /// </summary>
    public abstract class ProductoDecorator : ProductoBase
    {
        protected ProductoBase _producto;

        protected ProductoDecorator(ProductoBase producto)
        {
            _producto = producto;
        }
    }
}
